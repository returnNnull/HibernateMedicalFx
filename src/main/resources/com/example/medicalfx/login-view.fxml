<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" spacing="10.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.medicalfx.controllers.LoginController">
    <TextField fx:id="login" promptText="Login" />
    <PasswordField fx:id="pass" promptText="Password" />
    <Button mnemonicParsing="false" onMouseClicked="#onClick" text="Login">
        <padding>
            <Insets left="25.0" right="25.0" />
        </padding>
    </Button>
   <Label fx:id="error" text="Label" textFill="#b51f1f" visible="false" />
   <padding>
      <Insets left="10.0" right="10.0" />
   </padding>
</VBox>
